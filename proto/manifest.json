{
  "manifest_version": 3,
  "name": "Sub macro counter",
  "version": "1.0",
  "description": "Add the macros to all items and final tally",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": [
        "scripts/data.js",
        "scripts/content.js"
      ],
      "matches": [
        "https://www.subway.com/en-ca/restaurant/*/customizer/*"
      ]
    }
  ]
}